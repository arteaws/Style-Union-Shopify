ncode = zipCodeForm.querySelector(\"[name='pincode']\").value;\n            const skuID = zipCodeForm.querySelector(\"[name='sku_id']\").value;\n\n            if (!pincode) {\n                alert(\"Please fill in the zip code.\");\n                return; \/\/ Exit the function if zip code is not filled\n            }\n\n            \/\/ Disable button during processing\n            checkDeliveryButton.textContent = \"Checking...\";\n            checkDeliveryButton.disabled = true;\n\n            const data = {\n                location: pincode,\n  storePickup: \"YES\",\n              warehouseId: \"SHOPIFY\",\n                items: [{\n                    skuId: skuID,\n                    quantity: 1\n                }],\n                key: \"14c8f92d5ed852837e28a1081ec35ad18b9e10aeeb4a10fff076506489504272a6afaefbe9c65446961c85debbe5e9a4\"\n            };\n\n            axios.post(\"https:\/\/styleunion.online\/api\/style-union\/order\/checkServiceability\", data, {\n                    headers: {\n                        'Content-Type': 'application\/json'\n                    }\n                })\n                .then(response => {\n                    console.log(response.data);\n                    if (response.data.items[0].deliverymodes.length > 0) {\n                        \/\/ Extract the estimated delivery time in hours\n                        const deliveryTimeHours = response.data.items[0].deliverymodes[0].deliveryTime;\n\n                        \/\/ Calculate the delivery date\n                        const now = new Date();\n                        const deliveryDate = new Date(now.getTime() + deliveryTimeHours * 60 * 60 * 1000);\n\n                        \/\/ Format the delivery date\n                        const options = {\n                            weekday: 'long',\n                            month: 'long',\n                            day: 'numeric'\n                        };\n                        const formattedDeliveryDate = deliveryDate.toLocaleDateString('en-US', options);\n\n                        checkDeliveryButton.textContent = \"Check\";\n                        checkDeliveryButton.disabled = false;\n                        \/\/ Display delivery information\n                        responseMessage.textContent = `Estimated Delivery by ${formattedDeliveryDate}`;\n                        responseMessage.style.color = \"black\";\n                    } else if (!response.data.items[0].deliverymodes[0]) {\n                        \/\/ Display error message\n                        \/\/ alert(response.data.items[0].message);\n\n                        checkDeliveryButton.textContent = \"Check\";\n                        checkDeliveryButton.disabled = false;\n                        responseMessage.textContent = \"No delivery options available. Please try with the diffent pincode\";\n                        responseMessage.style.color = \"red\";\n\n\n                    }\n\n                })\n                .catch(error => {\n                    console.log(error);\n                    responseMessage.textContent = \"Invalid Pincode\";\n\n                    \/\/ Re-enable button\n                    checkDeliveryButton.textContent = \"Check\";\n                    checkDeliveryButton.disabled = false;\n                });\n        });\n    });\n<\/script>"}},"1af258e7-17dd-4fe5-bccb-351630d70821":{"type":"product_description","settings":{}},"ec330da2-623f-492b-97e6-ebb0396c1c58":{"type":"tab_content","disabled":true,"settings":{"tab_title":"Care Guide","tab_text":""}},"custom_liquid_NNRaAH":{"type":"custom_liquid","settings":{"custom_liquid":"<ul>\n{% if product.tags contains 'At 99'%}\n<li><strong>This product is non-returnable.<\/strong><\/li>\n{% else %}\n{% endif %}"}},"tab_content_UFPQqV":{"type":"tab_content","settings":{"tab_title":"More Info","tab_text":"<p>{{ product.metafields.custom.more_details | metafield_tag }}<\/p>"}}},"block_order":["judge_me_reviews_preview_badge_V