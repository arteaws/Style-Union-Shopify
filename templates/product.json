t responseMessage = document.getElementById(\"responseMessage\");\n        const checkDeliveryButton = document.getElementById(\"checkDeliveryButton\");\n        //document.getElementById(\"sku_id\").value = variant_first_SKU;\n\n\n        zipCodeForm.addEventListener(\"submit\", function(event) {\n            event.preventDefault(); // Prevent the default form submission behavior\n\n            const pincode = zipCodeForm.querySelector(\"[name='pincode']\").value;\n            const skuID = zipCodeForm.querySelector(\"[name='sku_id']\").value;\n\n            if (!pincode) {\n                alert(\"Please fill in the zip code.\");\n                return; // Exit the function if zip code is not filled\n            }\n\n            // Disable button during processing\n            checkDeliveryButton.textContent = \"Checking...\";\n            checkDeliveryButton.disabled = true;\n\n            const data = {\n                location: pincode,\n  storePickup: \"YES\",\n              warehouseId: \"SHOPIFY\",\n                items: [{\n                    skuId: skuID,\n                    quantity: 1\n                }],\n                key: \"14c8f92d5ed852837e28a1081ec35ad18b9e10aeeb4a10fff076506489504272a6afaefbe9c65446961c85debbe5e9a4\"\n            };\n\n            axios.post(\"https://styleunion.online/api/style-union/order/checkServiceability\", data, {\n                    headers: {\n                        'Content-Type': 'application/json'\n                    }\n                })\n                .then(response => {\n                    console.log(response.data);\n                    if (response.data.items[0].deliverymodes.length > 0) {\n                        // Extract the estimated delivery time in hours\n                        const deliveryTimeHours = response.data.items[0].deliverymodes[0].deliveryTime;\n\n                        // Calculate the delivery date\n                        const now = new Date();\n                        const d